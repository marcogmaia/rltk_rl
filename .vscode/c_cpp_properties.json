{
    "configurations": [
        {
            "name": "roguelike",
            "includePath": [
                // "/usr/include",
                // "/usr/include/entt",
                // "/usr/include/spdlog",
                // "/usr/include/c++/10.2.0",
                // "/usr/include/c++/10.2.0/tr1",
                // "/usr/include/c++/10.2.0/x86_64-pc-linux-gnu/",
                // "/usr/include/linux",
                "${workspaceFolder}/src",
                "${workspaceFolder}/lib/rltk"
            ],
            "browse": {
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db",
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "/usr/include/c++/**",
                    "/usr/include/linux/**",
                    "${workspaceFolder}/lib/rltk/**",
                    "${workspaceFolder}/src/**"
                ]
            },
            "defines": [
                "SPDLOG_FMT_EXTERNAL"
            ],
            "compilerPath": "/usr/bin/clang-11",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}
