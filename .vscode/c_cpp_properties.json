{
    "configurations": [
        {
            "name": "roguelike",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/lib/rltk"
            ],
            "browse": {
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db",
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "/usr/include/c++/**",
                    "/usr/include/linux/**",
                    "${workspaceFolder}/lib/rltk/**",
                    "${workspaceFolder}/src/**"
                ]
            },
            "defines": [
                "SPDLOG_FMT_EXTERNAL"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}
